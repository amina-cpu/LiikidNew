{
  "expo": {
    "name": "MarketplaceApp",
    "slug": "MarketplaceApp",
    "scheme": "marketplaceapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#16A085"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amina0909.MarketplaceApp"
    },
    "android": {
      "package": "com.amina0909.MarketplaceApp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#16A085"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "marketplaceapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "amina0909",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "94fdf34c-252c-4248-afb9-bd8bb87cae58"
      }
    }
  }
}